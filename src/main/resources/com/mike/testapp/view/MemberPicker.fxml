<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="256.0" prefWidth="500.0" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mike.testapp.MemberPickerController">
   <children>
      <Label text="Multiple matches found">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label text="Please select a member from the list below" />
      <ListView fx:id="lvMemberlist" prefHeight="190.0" prefWidth="389.0" />
      <HBox alignment="CENTER" prefHeight="49.0" prefWidth="500.0">
         <children>
            <Button fx:id="btnSelect" defaultButton="true" mnemonicParsing="false" onAction="#handleSelect" prefHeight="25.0" prefWidth="100.0" text="Select">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
